

title Escan near (101) 1.5K
mscan qh 1 0 qk 0 0 ql 1.15 0 en -1 0.5 31 monitor 600000


hset /sics/tc1/sensor/setpoint1 20
wait 60
hset /sics/tc1/sensor/setpoint1 40
wait 60
hset /sics/tc1/sensor/setpoint1 60
wait 60
hset /sics/tc1/sensor/setpoint1 80
wait 600

title QL scan near (101) En=1.5meV  80K
mscan qh 1 0 qk 0 0 ql  0.8 0.02 en 1.5 0 21 monitor 600000

title QL scan near (101) En=3meV  80K
