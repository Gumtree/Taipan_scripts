
# Q_scan T[110] (-q, +q, 6) BZ

m2 softlowerlim 32.5
s2 softlowerlim -116

m2 fixed -1
m1 fixed -1 

#(006) en = 3 meV

drive en 3.05
drive qh -0.2 qk 0.2 ql 6 en 3
m2 fixed 1
m1 fixed 1
bmonscan clear
bmonscan add qh -0.2 0.01 
bmonscan add qk -0.2 0.01
bmonscan run 41  monitor 500000
m2 fixed -1
m1 fixed -1

