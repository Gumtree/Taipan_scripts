
# (1.85 1.85 3.7)  2-15 meV

m2 softlowerlim 28.5
s2 softlowerlim -120

drive s2 -50
drive qh 1.85 qk 1.85 ql 3.7 en 10
runscan en 10 15 21 monitor 1500000

# (1.85 1.85 3.7) 15.25 - 25 meV

m2 softlowerlim 23
s2 softlowerlim -95

drive s2 -50
drive qh 1.85 qk 1.85 ql 3.7 en 15.25
runscan en 15.25 25 40 monitor 3000000

