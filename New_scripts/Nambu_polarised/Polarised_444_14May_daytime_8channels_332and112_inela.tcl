#HKL 3.0 3.0 2.0 9.25meV*4ch*2 (24min)
title 1.1 bar 3.0 3.0 2.0 9.25meV +-
drive qh 3.0 qk 3.0 ql 2.0 en 9.25
he3 polariser/spin 1
wait 2
he3 analyser/spin -1
wait 2
broadcast (3.0 3.0 2.0) +-
mscan qh 3.0 0 qk 3.0 0 ql 2.0 0 1 monitor 600000

title 1.1 bar 3.0 3.0 2.0 9.25meV -+
drive qh 3.0 qk 3.0 ql 2.0 en 9.25
he3 polariser/spin -1
wait 2
he3 analyser/spin 1
wait 2
broadcast (3.0 3.0 2.0) -+
mscan qh 3.0 0 qk 3.0 0 ql 2.0 0 1 monitor 600000

title 1.1 bar 3.0 3.0 2.0 9.25meV ++
drive qh 3.0 qk 3.0 ql 2.0 en 9.25
he3 polariser/spin 1
wait 2
he3 analyser/spin 1
wait 2
broadcast (3.0 3.0 2.0) ++
mscan qh 3.0 0 qk 3.0 0 ql 2.0 0 1 monitor 600000

title 1.1 bar 3.0 3.0 2.0 9.25meV --
drive qh 3.0 qk 3.0 ql 2.0 en 9.25
he3 polariser/spin -1
wait 2
he3 analyser/spin -1
wait 2
broadcast (3.0 3.0 2.0) --
mscan qh 3.0 0 qk 3.0 0 ql 2.0 0 1 monitor 600000

